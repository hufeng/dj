{
  "target": "es2017",
  "module": "commonjs",
  "strict": true,

  "allowUnreachableCode": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,

  "skipDefaultLibCheck": true,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "forceConsistentCasingInFileNames": true
}
